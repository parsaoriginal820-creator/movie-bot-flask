{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "app.py"
    },
    {
      "src": "/webhook",
      "dest": "app.py"
    },
    {
      "src": "/set_webhook",
      "dest": "app.py"
    }
  ]
}
